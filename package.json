{
  "name": "langchainjs",
  "version": "0.0.4",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "langchain",
    "examples",
    "docs",
    "test-exports-esm",
    "test-exports-cjs",
    "test-exports-cf",
    "test-exports-cra",
    "test-exports-vercel",
    "test-exports-vite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hwchase17/langchainjs.git"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "ci": "turbo run ci --filter=test-exports-vercel",
    "build": "turbo run build --filter=test-exports-vercel",
    "format": "turbo run format --filter=test-exports-vercel",
    "format:check": "turbo run format:check --filter=test-exports-vercel",
    "lint": "turbo run lint --filter=test-exports-vercel",
    "lint:fix": "yarn lint -- --fix",
    "test": "turbo run test test:integration --filter=test-exports-vercel && yarn test:exports:docker",
    "test:unit": "turbo run test",
    "test:int": "turbo run test:integration",
    "test:exports:docker": "docker compose up",
    "publish": "bash scripts/release-branch.sh && turbo run build lint test test:integration && yarn run test:exports:docker && yarn workspace langchain run release && echo 'ðŸ”— Open https://github.com/hwchase17/langchainjs/compare/release?expand=1 and merge the release PR'",
    "example": "turbo run start --filter test-exports-vercel",
    "prepare": "husky install",
    "precommit": "turbo run precommit",
    "docs": "yarn workspace docs start"
  },
  "author": "LangChain",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "langchain": "workspace:^",
    "turbo": "^1.7.4"
  },
  "resolutions": {
    "dpdm@^3.12.0": "patch:dpdm@npm%3A3.12.0#./.yarn/patches/dpdm-npm-3.12.0-0dfdd8e3b8.patch"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ],
    "*.md": "prettier --write"
  }
}
